        "build-calendar-prod": "cd nova-components/Calendar && npm run prod"
    },
    "devDependencies": {
        "axios": "^1.1.2",
        "laravel-mix": "^6.0.49"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.6",
        "@fullcalendar/daygrid": "^6.1.6",
        "@fullcalendar/interaction": "^6.1.6",
        "@fullcalendar/vue3": "^6.1.6",
        "moment": "^2.29.4",
        "vue-loader": "^17.0.1"
    }
}
